{% extends "KnpIpsumBundle::layout.html.twig" %}

{% block title "Symfony Forms Tutorial" %}

{% block content %}
    <h1>A sample contact form</h1>

    <form action="" method="POST" {{ form_enctype(form) }}>
        {{ form_errors(form) }}

        {{ form_rest(form) }}
        <input type="submit" value="Send" />
    </form>
{% endblock %}

{% set code = code(_self) %}
